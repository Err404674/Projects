        print(value)
    end
})

local equiper = "Equip"
local unequiper = "Unequip"

local function lowcooldown()
    local player = game.Players.LocalPlayer
    local equippedWeaponName = tostring(player.EquippedWeapon.Value)
    local character = player.Character
    local backpack = player.Backpack

    local weapon = character:FindFirstChild(equippedWeaponName) or backpack:FindFirstChild(equippedWeaponName)

    if weapon and weapon.Parent == character and weapon:IsA("Tool") then
        weapon.Activated:Connect(function()
        task.wait(1.15)
        game:GetService("ReplicatedStorage").EquipOrUnequipItems:InvokeServer(equippedWeaponName, "Weapon", unequiper)   
        task.wait()
        game:GetService("ReplicatedStorage").EquipOrUnequipItems:InvokeServer(equippedWeaponName, "Weapon", equiper)
        task.wait(0.1)
        character.Humanoid:EquipTool(weapon)
        end)
    end
end

Toggles.NoCD:OnChanged(function()
    repeat
    wait(0.01)
    lowcooldown()
    until Toggles.NoCD.Value == false
end)
